<h3>Patients</h3>
<a href="/patients/create" class="btn btn-secondary">Add</a>
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">SSN</th>
      <th scope="col">Full Name</th>
      <th scope="col">Age</th>
      <th scope="col">Phone Number</th>
      <th scope="col">City</th>
      <th scope="col">Doctor</th>
      <th scope="col">Actions</th>
    </tr>
  </thead>
  <tbody>
    <% patients.forEach(patient => { %>
    <tr>
      <th scope="row"><%= patient.ssn %></th>
      <td><%= patient.full_name %></td>
      <td><%= patient.age %></td>
      <td><%= patient.telephone %></td>
      <td><%= patient.city %></td>
      <td><%= patient.doctor ? patient.doctor.full_name : 'N/A' %></td>
      <td>
        <a href="/patients/<%= patient._id %>" class="btn btn-warning">Edit</a>
        <button type="button" onclick="deletePatient(`<%= patient._id %>`)" class="btn btn-danger">Delete</button>
      </td>
    </tr>
    <% }) %>
  </tbody>
</table>