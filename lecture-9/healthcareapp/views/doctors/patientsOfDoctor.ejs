<a  href="/doctors" type="button" class="btn btn-warning">Go Back</a>
<h1><%= doctors.full_name %></h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Name of Patient</th>
      <th scope="col">SSN of Patient</th>
      <th scope="col">Age of Patient</th>
      <th>City of Patient</th>
      <th>Telephone of Patient</th>
      <th scope="col">Actions</th>
    </tr>
  </thead>
  <tbody>
 <% patients.forEach(patient => { %>
  <% if (doctors._id.equals (patient.doctor)) { %>
    <tr>
      <th><%= patient.full_name %></th>
      <th><%= patient.ssn %></th>
      <th><%= patient.age %></th>
      <th><%= patient.city %></th>
      <th><%= patient.telephone %></th>
      <th>  <a href='/patients/<%= patient._id %>' class="btn btn-warning">View patient</a></th>
        </tr>
  <% } %>
 
  
 <% }) %>
  </tbody>
</table>